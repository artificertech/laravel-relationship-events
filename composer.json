{
    "name": "artificertech/laravel-relationship-events",
    "description": "Missing relationship events for Laravel",
    "homepage": "https://github.com/artificertech/laravel-relationship-events",
    "license": "MIT",
    "keywords": [
        "laravel",
        "relationship",
        "relations",
        "events"
    ],
    "authors": [
        {
            "name": "Cole Shirley",
            "email": "cole.shirley@artificertech.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/container": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/events": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^9.3|^9.5",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Artificertech\\RelationshipEvents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artificertech\\RelationshipEvents\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Artificertech\\RelationshipEvents\\RelationshipEventsServiceProvider"
            ]
        }
    }
}